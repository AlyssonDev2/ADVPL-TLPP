Static Function GerarCodigo()

    Local cCodigo
    DBSelectArea("SZ2")
    DBGOTO(1)
    DO WHILE !EOF()
        cCodigo := SZ2->Z2_COD
        DBSKIP()
    ENDDO
        nCodigo := Val(cCodigo)
        nCodigoConvertido := REPLICATE("0", 6-len(cValtoChar(nCodigo)))+cValtoChar(ncodigo+1)
    DbCloseArea()

Return nCodigoConvertido
Static Function fMarcarTodos(lCheck)

    if lCheck
        lCheck1 := .T.
        lCheck2 := .T.
        lCheck3 := .T.
        lCheck4 := .T.

        nCount := 4
    else
        lCheck1 := .F.
        lCheck2 := .F.
        lCheck3 := .F.
        lCheck4 := .F.

        nCount := 0
    endif

    oCheck1:Refresh()
    oCheck2:Refresh()
    oCheck3:Refresh()
    oCheck4:Refresh()

Return

